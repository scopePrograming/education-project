//- Start courses cat
section(class = 'courses-section')
    div.container-fluid
        h2(class = 'sub-title') Courses
        div(class = 'row')
            div(class = 'col-lg-12 col-md-6')
                div(class = 'menu-box text-center')
                    ul(class = 'list-courses')
                        li(class = 'mixitup-control-active' data-filter = '.course-web') Web
                        li(data-filter = '.course-en') En 
                        li(data-filter = '.course-ar') Ar 
                        li(data-filter = '.course-web') Web 
                        li(data-filter = '.course-math') Math 
        div(class = 'row box_list')
            - for (var x = 3; x <=5; x++)
                div(class = 'col-lg-4 col-md-6 mix course-math')
                    div(class="card")
                        img(src=`./assets/images/pic${x}.svg` class="card-img-top" alt="image" '')/
                        div(class="card-body")
                            h5(class="card-title mb-0") Course Math#{x}
                            span(class = 'text') Instructor Name
                            p(class="card-text py-2") Some quick example text to build on the card title and make up the bulk of the card's content.
                            div(class = 'details d-flex justify-content-between')
                                div(class = 'time')
                                    span(class = 'fas fa-clock') 
                                    span(class = 'ml-2') 45 mins
                                div(class = 'file')
                                    span(class = 'fas fa-file-alt') 
                                    span(class = 'ml-2') 4 files
                            div(class = 'price mt-3') $75.99
            - var coursesNameEn = ['En1', 'En2']
                each course in coursesNameEn
                    div(class = 'col-lg-4 col-md-6 mix course-en')
                        div(class="card")
                            img(src=`./assets/images/pic1.svg` class="card-img-top" alt="image" '')/
